	public void refreshtableClientesVendaAtacado() {
		try {
			String query = "SELECT clientes.Cliente, todosprodutos.Produto from clientes" "join compra" "on clientes.ID = compra.idCliente" "join todosprodutos" "on todosprodutos.Id = compra.idProduto" "order by clientes.Cliente;";
			PreparedStatement pst = conexao.prepareStatement(query);
			ResultSet rs = pst.executeQuery();
			tableClientesVendaAtacado.setModel(DbUtils.resultSetToTableModel(rs));

			pst.close();
			rs.close();

		} catch (Exception g) {
			g.printStackTrace();
		}
	}
	
	
	
					pst.setTimestamp(3, new java.sql.Timestamp(java.util.Calendar.getInstance().getTimeInMillis()));